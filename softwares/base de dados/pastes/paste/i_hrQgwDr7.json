 "text": "<!DOCTYPE html>\n<head xmlns=\"http://www.w3.org/1999/html\">\n    <title>Devents</title>\n    <!-- Compiled and minified CSS -->\n    <link rel=\"stylesheet\" href=\"https://cdnjs.cloudflare.com/ajax/libs/materialize/0.100.2/css/materialize.min.css\">\n     <link href=\"https://fonts.googleapis.com/icon?family=Material+Icons\" rel=\"stylesheet\">\n</head>\n<body>\n    <nav class=\"blue\" class=\"blue\">\n        <div class=\"nav-wrapper\">\n            <div class=\"brand-logo\">&nbsp;&nbsp;&nbsp;Devent</div>\n            <ul id=\"nav-mobile\" class=\"right hide-on-med-and-down\">\n                <li><a href=\"/accounts/profile\" class=\"top\"></a></li>\n                <li><a href=\"/accounts/sign_up_room\" class=\"top\"> </a></li>\n                <li><a href=\"/login\"></a></li>\n            </ul>\n        </div>\n    </nav>\n    <form method=\"post\">\n            {% csrf_token %}\n     <div class=\"row\">\n        <div class=\"col s12 m1000\">\n          <div class=\"card\">\n            <div class=\"card-image\">\n              <img src=\"{{ image_path }}\">\n            </div>\n            <div class=\"card-content\">\n              <input id=\"room_name\" name=\"room_name\" type=\"text\" class=\"validate\" value=\"{{room.name}}\" placeholder=\"\">\n              <input id=\"discription\" name=\"discription\" type=\"text\" class=\"validate\" value=\"{{room.cmt}}\" placeholder=\"\">\n            </div>\n          </div>\n        </div>\n      </div>\n        <input class=\"waves-effect waves-light btn\" type=\"submit\" value=\" \">\n    <div class=\"card\">\n    <div class=\"card-tabs\">\n      <ul class=\"tabs tabs-fixed-width\">\n        <li class=\"tab\"><a class=\"active\" href=\"#members\"><i class=\"material-icons center\">person</i></a></li>\n        <li class=\"tab\"><a href=\"#tasks\"></a></li>\n        <li class=\"tab\"><a href=\"#events\"></a></li>\n        <li class=\"tab\"><a href=\"#admins\"></a></li>\n      </ul>\n    </div>\n    <div class=\"card-content grey lighten-4\">\n      <div id=\"events\">\n          <ul class=\"collapsible\" data-collapsible=\"accordion\">\n            {% for event in events%}\n                <li>\n                    <div class=\"collapsible-header\">\n                        <a>\n                            {{event}}#id{{event.id}}\n                            <br>\n                            <i class=\"material-icons\">alarm</i>{{event.date}}\n                        </a>\n                    </div>\n                    <div class=\"collapsible-body\">\n                        <span>{{ event.cmt }}</span>\n                        <br>\n                        <a href=\"/accounts/event/{{event.id}}\">>></a>\n                    </div>\n                </li>\n            {% endfor %}\n          </ul>\n          {% if admin %}\n                <a onclick=\"location.href = '/accounts/room/sign_up_event/{{room.id}}'\" class=\"btn-floating btn-large waves-effect waves-light yellow\"><i class=\"material-icons\">add</i></a>\n            {% endif %}\n      </div>\n        <div id=\"tasks\">\n          <ul class=\"collapsible\" data-collapsible=\"accordion\">\n            {% for task in tasks%}\n                <li>\n                    <div class=\"collapsible-header\">\n                        <a>\n                            {{task}}#id{{task.id}}\n                            <br>\n                            <i class=\"material-icons\">alarm</i>{{task.date}}\n                        </a>\n                    </div>\n                    <div class=\"collapsible-body\">\n                        <span>{{ task.cmt }}</span>\n                        <br>\n                        <a href=\"/accounts/task/{{task.id}}\">>></a>\n                    </div>\n                </li>\n            {% endfor %}\n          </ul>\n          {% if admin %}\n                <a onclick=\"location.href = '/accounts/room/sign_up_task/{{room.id}}'\" class=\"btn-floating btn-large waves-effect waves-light yellow\"><i class=\"material-icons\">add</i></a>\n            {% endif %}\n      </div>\n      <div id=\"members\">\n          <div>\n              <ul class=\"collapsible\" data-collapsible=\"accordion\">\n                    <li>\n                    </li>\n                  {% for item in data%}\n                        <li data-id=\"{{item.1.id}}\" class=\"member\">\n                            <div class=\"collapsible-header\">\n                                <a href=\"/accounts/member/{{item.1.id}}\">{{item.1}}#id{{item.1.id}}</a>\n                                <i class=\"material-icons right delete-item\" >clear</i>\n                            </div>\n\n                                <div class=\"collapsible-body\">\n                                    {% for user_task in item.0 %}\n                                        <div class=\"collection\">\n                                            <a href=\"/accounts/task/{{user_task.id}}\" class=\"collection-item\">\n                                                {{user_task}}\n                                                <br>\n                                                <i class=\"material-icons center\">alarm</i>{{user_task.date}}\n                                            </a>\n                                        </div>\n                                    {% endfor %}\n                                    <br>\n                                        &nbsp;&nbsp;&nbsp;<a onclick=\"location.href = '/accounts/room/sign_up_event_on_user/{{item.1.id}}/{{ room.id }}/'\" class=\"btn-floating btn-large waves-effect waves-light yellow\"><i class=\"material-icons\">add</i></a>\n                                </div>\n                        </li>\n                  {% endfor %}\n              </ul>\n          </div>\n            {% if admin %}\n                <input name=\"id_\" class=\"collection-item\">\n                <button type=\"submit\" class=\"waves-effect waves-light btn yellow\">Add</button> <font color=\"white\">{{error}}</font>\n            {% endif %}\n        </form>\n      </div>\n      <div id=\"admins\">\n          <div class=\"collection\">\n              {% for admin in admins %}\n                <a href=\"/accounts/member/{{admin.id}}\" class=\"collection-item\">{{admin}}</a>\n            {% endfor %}\n          </div>\n      </div>\n    </div>\n  </div>\n     <script type=\"text/javascript\" src=\"https://code.jquery.com/jquery-3.2.1.min.js\"></script>\n    <script src=\"https://cdnjs.cloudflare.com/ajax/libs/materialize/0.100.2/js/materialize.min.js\"></script>\n    <script>\n         $(document).ready(function(){\n            $('.collapsible').collapsible();\n          });\n    </script>\n    <script>\n        $('.fixed-action-btn').openFAB();\n        $('.fixed-action-btn').closeFAB();\n        $('.fixed-action-btn.toolbar').openToolbar();\n        $('.fixed-action-btn.toolbar').closeToolbar()\n    </script>\n<script>\n        $(\".dropdown-button\").dropdown();\n        $('.delete-item').on('click', function(){\n            var $this = $(this);\n            var _id = $this.parents('li.member').data('id');\n            $.ajax({\n                url: '/URL/admin/delete/' + _id,\n                success: function(res){\n                    console.log(res);\n                    if (res == 1){\n                        $this.parents('li.member').remove()\n                    }else{\n                        alert('ERROR');\n                    }\n                }\n            })\n        })\n    </script>\n</body>\n</html>",
