ion math(val)\n{\n    document.getElementById(\"display\").value+=val;\n}\nfunction equals()\n{\n    try\n    {\n      clear(eval(document.getElementById(\"display\").value))\n    }\n    catch(equals)\n    {\n      clear('Error')\n    }\n}\n</script>\n</head>\n<body>\n<div class=\"box\">\n    <div class=\"display\"><input type=\"text\" size=\"15.75\" id=\"display\" readonly>\n    </div>\n    <div class=\"keys\">\n        <p>\n        <input type=\"button\" class=\"button gray\" value=\"%\" onclick='math(\"%\")'>\n        <input type=\"button\" class=\"button gray\" value=\"(\" onclick='math(\"(\")'>\n        <input type=\"button\" class=\"button gray\" value=\")\" onclick='math(\")\")'>\n        <input type=\"button\" class=\"button red\" value=\"/\" onclick='math(\"/\")'>\n        </p>\n        <p>\n        <input type=\"button\" class=\"button black\" value=\"7\" onclick='math(\"7\")'>\n        <input type=\"button\" class=\"button black\" value=\"8\" onclick='math(\"8\")'>\n        <input type=\"button\" class=\"button black\" value=\"9\" onclick='math(\"9\")'>\n        <input type=\"button\" class=\"button red\" value=\"*\" onclick='math(\"*\")'>\n        </p>\n        <p>\n        <input type=\"button\" class=\"button black\" value=\"4\" onclick='math(\"4\")'>\n        <input type=\"button\" class=\"button black\" value=\"5\" onclick='math(\"5\")'>\n        <input type=\"button\" class=\"button black\" value=\"6\" onclick='math(\"6\")'>\n        <input type=\"button\" class=\"button red\" value=\"-\" onclick='math(\"-\")'>\n        </p>\n        <p>\n        <input type=\"button\" class=\"button black\" value=\"1\" onclick='math(\"1\")'>\n        <input type=\"button\" class=\"button black\" value=\"2\" onclick='math(\"2\")'>\n        <input type=\"button\" class=\"button black\" value=\"3\" onclick='math(\"3\")'>\n        <input type=\"button\" class=\"button red\" value=\"+\" onclick='math(\"+\")'>\n        </p>\n        <p>\n        <input type=\"button\" class=\"button black\" value=\"0\" onclick='math(\"0\")'>\n        <input type=\"button\" class=\"button black\" value=\".\" onclick='math(\".\")'>\n        <input type=\"button\" class=\"button black\" value=\"C\" onclick='clear(\"\")'>\n        <input type=\"button\" class=\"button orange\" value=\"=\" onclick='equals()'>\n        </p>\n    </div>\n</div>\n \n</body>\n</html>",
